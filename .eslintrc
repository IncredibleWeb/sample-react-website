{
  "extends": [ "prettier" ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": false
  },
  "globals": {
    "document": true,
    "window": true,
    "module": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ]
}
